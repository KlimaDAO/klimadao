    "jest": "^29.6.1",
    "lodash": "^4.17.21",
    "lodash.memoize": "^4.1.2",
    "newman": "^5.3.2",
    "nock": "^13.3.2",
    "node-fetch": "^2.6.12",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5"
  }
