{
  "name": "@klimadao/app",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "prettier --check . && npm run lingui:compile && next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lingui:extract": "node ../scripts/lingui.js extract --clean --overwrite",
    "lingui:compile": "lingui compile",
    "lingui:extract:dev": "node ../scripts/lingui.js extract --clean --overwrite --locale=en && node ../scripts/lingui.js extract --clean --overwrite --locale=en-pseudo",
    "postinstall": "npm run lingui:compile"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@lingui/core": "^3.13.0",
    "@lingui/react": "^3.13.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/material": "^5.2.3",
    "@reduxjs/toolkit": "^1.7.0",
    "@tippyjs/react": "^4.2.6",
    "@toruslabs/torus-embed": "^1.22.6",
    "@transak/transak-sdk": "^1.0.31",
    "@translation/lingui": "^1.0.0",
    "ethers": "^5.7.0",
    "next": "^12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.1.0",
    "react-router-dom": "^6.1.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.2",
    "@lingui/cli": "^3.13.0",
    "@lingui/macro": "^3.13.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.18.1"
  }
}
