{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": ".",
    "paths": {
      "#lib/*": ["../lib/*"],
      "src/*": ["./src/*"]
    }
  },
  "include": ["src/**/*.ts"]
}
