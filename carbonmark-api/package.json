{
  "name": "carbonmark-api-ts",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js",
    "dev:watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev\"",
    "start": "npm run build:ts && fastify start -l info dist/src/app.js",
    "start:test": "npm run build:ts && fastify start -l info dist/src/app.js --port 3000",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "test:postman": "./postman_tests.sh",
    "build": "rm -rf ./dist && npm run build:ts",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "generate:types": "rm -rf ./.generated/types && graphql-codegen --config src/graphql/codegen.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/jwt": "^6.7.1",
    "@fastify/response-validation": "^2.3.1",
    "@fastify/sensible": "^5.0.0",
    "@fastify/session": "^10.3.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/type-provider-typebox": "^3.1.0",
    "@mgcrea/fastify-session": "^1.1.0",
    "@now-ims/fastify-firebase": "^1.1.3",
    "@sanity/client": "^6.1.2",
    "@sinclair/typebox": "^0.28.5",
    "dotenv": "^16.1.4",
    "ethers": "^5.7.2",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-lcache": "^1.1.1",
    "fastify-plugin": "^4.0.0",
    "firebase-admin": "^11.7.0",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "concurrently": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "fastify-tsconfig": "^1.0.1",
    "lodash": "^4.17.21",
    "newman": "^5.3.2",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
