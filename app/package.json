{
  "name": "@klimadao/app",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "prettier --check . && npm run lingui:compile && next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lingui:extract": "node ../scripts/lingui.js extract --clean --overwrite",
    "lingui:compile": "lingui compile",
    "lingui:extract:dev": "node ../scripts/lingui.js extract --clean --overwrite --locale=en && node ../scripts/lingui.js extract --clean --overwrite --locale=en-pseudo",
    "postinstall": "npm run lingui:compile"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@formkit/auto-animate": "^1.0.0-beta.5",
    "@lingui/core": "^3.15.0",
    "@lingui/react": "^3.15.0",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "^5.10.14",
    "@reduxjs/toolkit": "^1.9.0",
    "@tippyjs/react": "^4.2.6",
    "@translation/lingui": "^1.0.0",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "next": "^13.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.5",
    "@lingui/cli": "^3.15.0",
    "@lingui/macro": "^3.15.0",
    "@next/bundle-analyzer": "^13.1.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "typescript": "^4.9.4"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.18.1"
  }
}
