{
  "name": "@klimadao/app",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "prettier --check . && npm run lingui:compile && next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lingui:extract": "node ../scripts/lingui.js extract --clean --overwrite",
    "lingui:compile": "lingui compile",
    "lingui:extract:dev": "node ../scripts/lingui.js extract --clean --overwrite --locale=en && node ../scripts/lingui.js extract --clean --overwrite --locale=en-pseudo",
    "postinstall": "npm run lingui:compile"
  },
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@lingui/core": "^3.17.2",
    "@lingui/react": "^3.17.2",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/nextjs": "^7.50.0",
    "@tippyjs/react": "^4.2.6",
    "@translation/lingui": "^1.0.0",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "next": "^13.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.6",
    "@lingui/cli": "^3.17.2",
    "@lingui/macro": "^3.17.2",
    "@next/bundle-analyzer": "^13.3.1",
    "@types/node": "^18.16.2",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.18.1"
  }
