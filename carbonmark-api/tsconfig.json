{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": ".",
    "paths": {
      "*": ["./*"],
      "@klimadao/*": ["../*"]
    }
  },
  "include": ["src/**/*.ts", "scripts/**/*.ts"]
}
