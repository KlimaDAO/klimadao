    "@hookform/resolvers": "^2.8.8",
    "@lingui/core": "^3.13.0",
    "@lingui/react": "^3.13.0",
    "@portabletext/react": "^1.0.6",
    "@sanity/client": "^3.3.2",
    "@tippyjs/react": "^4.2.6",
    "@translation/lingui": "^1.0.0",
    "ethers": "^5.7.2",
    "firebase-admin": "^11.2.1",
    "jspdf": "^2.5.1",
