{
  "buildCommand": "npm run build",
  "outputDirectory": "./dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev:watch",
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/serverless.ts"
    }
  ]
}
